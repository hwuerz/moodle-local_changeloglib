<XMLDB xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" PATH="local/changeloglib/db" VERSION="20170709" COMMENT="Upload notification" xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
  <TABLES>
    <TABLE NAME="local_changeloglib_backup" COMMENT="All files for which a backup was created">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="context" TYPE="int" LENGTH="6" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="scope" TYPE="int" LENGTH="6" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="data" TYPE="char" LENGTH="800" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timestamp" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>